{
  "name": "itech/dbal-bundle",
  "description": "Dbal bundle",
  "type": "symfony-bundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "aleksandr.k",
      "email": "aleksandr.k@itechpsp.com"
    }
  ],
  "require": {
    "php": "~8.2",
    "ext-pdo": "*",
    "ext-json": "*",
    "doctrine/doctrine-bundle": "^2.14",
    "symfony/config": "^7.2",
    "symfony/dependency-injection": "^7.2",
    "symfony/http-kernel": "^7.2",
    "doctrine/dbal": "^4.2",
    "jolicode/automapper": "^9.3",
    "symfony/serializer": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "ITech\\DbalBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ITech\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "friendsofphp/php-cs-fixer": "^3.74",
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "zenstruck/foundry": "^2.3"
  },
  "scripts": {
    "cs:fix": "@php vendor/bin/php-cs-fixer fix"
  }
}
