{
  "name": "alexk136/dbal-bundle",
  "description": "High-performance DBAL operations bundle for Symfony with support for bulk insert, update, upsert, delete and streaming queries.",
  "type": "symfony-bundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "Alex",
      "email": "alexk@elrise.ru"
    }
  ],
  "require": {
    "php": "~8.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "doctrine/dbal": "^4.2",
    "doctrine/doctrine-bundle": "^2.14",
    "symfony/config": "^7.2",
    "symfony/dependency-injection": "^7.2",
    "symfony/http-kernel": "^7.2",
    "symfony/serializer": "^7.2",
    "symfony/uid": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "Elrise\\Bundle\\DbalBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elrise\\Bundle\\DbalBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "friendsofphp/php-cs-fixer": "^3.74",
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "zenstruck/foundry": "^2.3"
  },
  "scripts": {
    "cs:fix": "@php vendor/bin/php-cs-fixer fix"
  }
}
